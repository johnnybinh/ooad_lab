
@startuml
!theme plain

class Student{
name: string;
phoneNumber: string;
email: string;
}


class UndergraduateStudent{
  attribute:type
}

class GraduateStudent{
  attribute:type
}


interface StudentFactory{
  createStudent():void;
}

class Enrollment{
  attribute:type;
}


StudentFactory-down->Student:create;
UndergraduateStudent-up->Student
GraduateStudent-up->Student



@enduml